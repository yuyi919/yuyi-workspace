{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "id": "Format",
  "title": "",
  "type": "object",
  "properties": {
    "project": {
      "type": "string",
      "description": "The name of the project.",
      "alias": "p",
      "$default": {
        "$source": "projectName"
      },
      "x-prompt": "What is the name of the project for the generator?"
    },
    "builder": {
      "type": "string",
      "enum": [
        "auto",
        "tsdx",
        "tsc"
      ],
      "description": "构建使用的工具(不选则根据原始选项，如果原始不属于任何一种则中断操作)",
      "default": "auto"
    },
    "tags": {
      "type": "string",
      "description": "Add tags to the project (used for linting)",
      "alias": "t"
    }
  },
  "required": [
    "project"
  ]
}
