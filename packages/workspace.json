    "node-cli-utils": {
      "root": "libs/node/cli-utils",
      "sourceRoot": "libs/node/cli-utils/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "configurations": {
            "watch": {
              "commands": ["pnpm run build --watch"]
            }
          },
          "options": {
            "commands": ["pnpm run build"],
            "cwd": "libs/node/cli-utils"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/node/cli-utils/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/node/cli-utils"],
          "options": {
            "jestConfig": "libs/node/cli-utils/jest.config.js",
            "passWithNoTests": true
          }
        },
        "command": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {},
          "configurations": {
            "remove": {
              "cwd": ".",
              "commands": [
                {
                  "command": "nx generate @nrwl/workspace:remove --projectName=node-cli-utils --forceRemove"
                }
              ]
            },
            "format": {
              "cwd": ".",
              "commands": [
                {
                  "command": "nx generate @yuyi919/nx-plugin-workspace-helper:format --project=node-cli-utils --no-interactive"
                }
              ]
            }
          }
        }
      }
    },
