{
  "version": 2,
  "projects": {
    "workspace-base-rig": {
      "builder": "tsc",
      "root": "libs/tools/workspace-base-rig",
      "sourceRoot": "libs/tools/workspace-base-rig/src",
      "projectType": "library",
      "generators": {},
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/tools/workspace-base-rig/src/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/tools/workspace-base-rig/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/packages/shared"]
        },
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["pnpm run build"],
            "cwd": "libs/tools/workspace-base-rig"
          },
          "outputs": ["{options.cwd}"]
        },
        "dev": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["pnpm run dev"],
            "cwd": "libs/tools/workspace-base-rig"
          }
        },
        "command": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {},
          "configurations": {}
        }
      },
      "nxJsonSection": {
        "tags": ["internal", "lerna-package"]
      }
    },
