{
  "presets": [
    [
      "@babel/preset-typescript",
      {
        "allowNamespaces": false
      }
    ]
  ],
  "plugins": [
    [
      "const-enum",
      {
        "transform": "constObject"
      }
    ],
    [
      "import",
      {
        "libraryName": "lodash",
        "libraryDirectory": "",
        "camel2DashComponentName": false,
        "style": false
      }
    ]
  ]
}
