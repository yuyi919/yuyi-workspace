{
  "compileOnSave": false,
  "exclude": ["node_modules", "tmp"],
  "compilerOptions": {
    "sourceMap": true,
    "composite": true,
    "declarationMap": true,
    "incremental": true,
    "declaration": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "preserveSymlinks": false,
    "importHelpers": true,
    "rootDir": ".",
    "target": "es2015",
    "module": "esnext",
    "types": ["jest", "node"],
    "lib": ["dom", "es2020", "esnext", "es6"],
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@bot": ["apps/bot/src/index.ts"],
      "@bot/*": ["apps/bot/src/*"],
      "@core": ["libs/core/server/src/index.ts"],
      "@core/*": ["libs/core/server/src/*"],
      "@logger": ["libs/node/shared/logger/src/index.ts"],
      "@node-logger": ["libs/node/shared/logger/src/index.ts"],
      "@static-value": ["libs/shared/packages/static-values/src/index.ts"],
      "@types": ["libs/shared/packages/types/src/index.ts"],
      "@types/*": ["libs/shared/packages/types/src/*"],
      "@utils": ["libs/shared/packages/utils/src/index.ts"],
      "@utils/*": ["libs/shared/packages/utils/src/*"],
      "@yuyi919/vue-antv-plus2": ["libs/web/vue/antv-plus2/src/index.ts"],
      "@yuyi919/vue-antv-plus2-action": ["libs/web/vue/antv-plus2/packages/action/src/index.ts"],
      "@yuyi919/vue-antv-plus2-form": ["libs/web/vue/antv-plus2/packages/form/src/index.ts"],
      "@yuyi919/vue-antv-plus2-helper": ["libs/web/vue/antv-plus2/packages/helper/src/index.ts"],
      "@yuyi919/vue-antv-plus2-modal": ["libs/web/vue/antv-plus2/packages/modal/src/index.ts"],
      "@yuyi919/vue-antv-plus2-shared": ["libs/web/vue/antv-plus2/packages/shared/src/index.ts"],
      "@yuyi919/vue-antv-plus2-stable": ["libs/web/vue/antv-plus2/packages/stable/src/index.ts"],
      "@yuyi919/vue-antv-plus2-theme/index.less": [
        "libs/web/vue/antv-plus2/packages/theme/index.less"
      ],
      "@yuyi919/vue-antv-plus2-theme": ["libs/web/vue/antv-plus2/packages/theme/src/index.ts"],
      "@yuyi919/vue-jss": ["libs/web/vue/jss/src/index.ts"],
      "@yuyi919/vue-use": ["libs/web/vue/use/src/index.ts"]
    }
  }
}
