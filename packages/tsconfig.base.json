{
  "compileOnSave": false,
  "exclude": ["node_modules", "tmp"],
  "compilerOptions": {
    "sourceMap": true,
    "composite": true,
    "declarationMap": true,
    "incremental": true,
    "declaration": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "preserveSymlinks": false,
    "importHelpers": true,
    "rootDir": ".",
    "target": "es2015",
    "module": "esnext",
    "types": ["jest", "node"],
    "lib": ["dom", "es2020", "esnext", "es6"],
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@bot": ["apps/bot/src/index.ts"],
      "@bot/*": ["apps/bot/src/*"],
      "@core": ["packages/core/server/src/index.ts"],
      "@core/*": ["packages/core/server/src/*"],
      "@logger": ["packages/node/shared/logger/src/index.ts"],
      "@node-logger": ["packages/node/shared/logger/src/index.ts"],
      "@static-value": ["packages/shared/packages/static-values/src/index.ts"],
      "@types": ["packages/shared/packages/types/src/index.ts"],
      "@types/*": ["packages/shared/packages/types/src/*"],
      "@utils": ["packages/shared/packages/utils/src/index.ts"],
      "@utils/*": ["packages/shared/packages/utils/src/*"],
      "@yuyi919/vue-antv-plus2": ["packages/web/vue/antv-plus2/index.ts"],
      "@yuyi919/vue-antv-plus2-action-button": [
        "packages/web/vue/antv-plus2/packages/action-button/src/index.ts"
      ],
      "@yuyi919/vue-antv-plus2-form": ["packages/web/vue/antv-plus2/packages/form/src/index.ts"],
      "@yuyi919/vue-antv-plus2-helper": [
        "packages/web/vue/antv-plus2/packages/helper/src/index.ts"
      ],
      "@yuyi919/vue-antv-plus2-modal": ["packages/web/vue/antv-plus2/packages/modal/src/index.ts"],
      "@yuyi919/vue-antv-plus2-shared": [
        "packages/web/vue/antv-plus2/packages/shared/src/index.ts"
      ],
      "@yuyi919/vue-antv-plus2-stable": [
        "packages/web/vue/antv-plus2/packages/stable/src/index.ts"
      ],
      "@yuyi919/vue-antv-plus2-theme": ["packages/web/vue/antv-plus2/packages/theme/src/index.ts"],
      "@yuyi919/vue-antv-plus2-theme/index.less": [
        "packages/web/vue/antv-plus2/packages/theme/index.less"
      ]
    }
  }
}
